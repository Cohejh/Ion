{
  "name": "ion",
  "version": "1.1.0",
  "sideEffects": false,
  "description": "A fast, modern private browser.",
  "keywords": [
    "web-browser",
    "modern",
    "electron",
    "react",
    "private",
    "browser",
    "edge",
    "chrome",
    "firefox",
    "cluckcluckgo",
    "wexond",
    "google",
    "ion"
  ],
  "homepage": "https://github.com/Cohejh/Ion#readme",
  "main": "build/main.bundle.js",
  "author": "COHEJH <jcohenkadosh@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cohejh/Ion.git"
  },
  "bugs": {
    "url": "https://github.com/Cohejh/Ion/issues"
  },
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack\"",
    "ci-build": "node scripts/ci-build.js",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-win32": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m",
    "compile-linux": "npm run build && electron-builder -l",
    "compile-all" : "npm run build && electron-builder -mwl",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@cliqz/adblocker-electron": "1.23.8",
    "@electron/remote": "^2.0.8",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/animejs": "^3.1.3",
    "@types/chrome": "0.0.193",
    "@types/crypto-js": "^4.0.1",
    "@types/jszip": "^3.4.1",
    "@types/nedb": "1.8.12",
    "@types/node": "18.6.4",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/rimraf": "^3.0.0",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@wexond/rpc-core": "^1.0.3",
    "@wexond/rpc-electron": "^1.0.3",
    "animejs": "^3.2.1",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "electron": "20.0.1",
    "electron-builder": "23.3.3",
    "electron-extensions": "^7.0.0-beta.3",
    "electron-updater": "5.2.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "file-type": "17.1.6",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.3.2",
    "icojs": "^0.17.0",
    "jszip": "^3.10.1",
    "mobx": "6.3.2",
    "mobx-react-lite": "3.2.0",
    "nedb": "1.8.0",
    "node-bookmarks-parser": "^2.0.0",
    "node-fetch": "^3.2.10",
    "prettier": "2.7.1",
    "pretty-bytes": "6.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-refresh": "^0.14.0",
    "react-windows-controls": "1.1.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
